#include<stdio.h>
int main()
{
	int a[50],i,x,n;
	printf("Enter the number of elements: ");
	scanf("%d",&n);
	printf("Enter the elements:\n");
	for(i=0;i<n;i++)
		scanf("%d",&a[i]);
	printf("Enter tne searching element: ");
	scanf("%d",&x);
	for(i=0;i<n;i++)
		if(a[i]==x)
		{	
			printf("Successful search at position:%d",i+1);
			break;
		}
	if(i==n)	
		printf("Unsuccessful search");
	return 0;			
}
